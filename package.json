{
  "author": "Oscar Palm√©r",
  "description": "Things I've made: utilities, demos, experiments, and other whatchamacallits.",
  "devDependencies": {
    "@11ty/eleventy": "^3.1",
		"@biomejs/biome": "^1.9",
		"@oscarpalmer/atoms": "^0.102",
		"@oscarpalmer/oui": "^0.12",
		"@tabler/icons": "^3.34",
		"concurrently": "^9.1",
		"html-minifier": "^4",
		"markdown-it": "^14.1",
		"sass": "^1.89"
  },
  "license": "MIT",
  "homepage": "https://github.com/oscarpalmer/things.oscarpalmer.se",
  "name": "things.oscarpalmer.se",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oscarpalmer/things.oscarpalmer.se.git"
  },
  "scripts": {
    "build": "rm -rf ./build && npm run css:build && eleventy",
		"css:build": "sass ./source/assets/stylesheets/styles.scss ./build/assets/stylesheets/styles.css --style=compressed --quiet",
		"css:watch": "sass ./source/assets/stylesheets/styles.scss ./build/assets/stylesheets/styles.css --quiet --watch",
		"serve": "rm -rf ./build && concurrently \"npm run css:watch\" \"eleventy --quiet --serve\""
  },
	"type": "module",
  "version": "25.6.22"
}
